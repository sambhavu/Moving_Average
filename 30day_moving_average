void MA :: solve(){ 
 //thirty day moving average 
 
 double count = 0; 
 double sum = 0; 
 double MA30; 
 
 for(int i = 0; i<63; i ++){
  count++;
  if(count>=30){
      int j = i-30;
      sum=0;
      for(j; j<count; j++)
      {
        sum+=Data[j];
      }
      MA30 = sum/30;
      cout<<MA30<<"\n";
      
  }
 }
  
    
    
} 
int main()
{
    
    MA x; 
    x.solve();
return 0;
}
